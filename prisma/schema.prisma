generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model VendeCarro {
  id             Int    @id @default(autoincrement()) // Adicionando uma chave prim√°ria
  manufactureYear Int
  brand          String
  model          String
  horsepower     Int
  engine         String
}
